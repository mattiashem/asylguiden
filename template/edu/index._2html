			<console>

                <div class="row">
                    <div class="col-md-8">
                        {% if user.is_authenticated %}
                        <b>User {{ user.username}}</b><a href="/users/mypage">{% trans "Account" %}</a> <a href="/users/logout">{% trans "Log Out" %}</a>
                        {% else %}
                        <a href="/users/login"><button class="btn btn-primary" type="submit">{% trans "Login" %}</button></a><a href="/users/register"><button class="btn btn-success" type="submit">{% trans "Register" %}</button></a>
                        {% endif %}
                    </div>
                     <div class="col-md-4">
                        <form action="{% url 'set_language' %}" method="post" class="form-inline" >
                        <b>{% trans "Language" %}</b>
                        {% csrf_token %}
                        <input   name="next" type="hidden" value="{{ redirect_to }}" class="form-control"/>
                        <select  name="language" class="form-control"
                            {% get_language_info_list for LANGUAGES as languages %}
                            {% for language in languages %}
                                <option  value="{{ language.code }}"{% if language.code == LANGUAGE_CODE %} selected="selected"{% endif %}>
                                    {{ language.name_local }} ({{ language.code }})
                                </option>
                            {% endfor %}
                        </select>
                        <input type="submit" value="Change" class="form-control btn btn-info"/>
                        </form></div>
                    </div>








